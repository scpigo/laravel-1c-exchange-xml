{
    "name": "scpigo/laravel-1c-exchange-xml",
    "description": "Package for maintaining xml file exchange between laravel and 1C",
    "type": "library",
    "require": {
        "php": "^7.3|^8.0",
        "laravel/framework": "^8.0",
        "mtownsend/xml-to-array": "^2.0",
        "spatie/array-to-xml": "^2.16",
        "scpigo/laravel-system-job": "@dev",
        "scpigo/core": "@dev",
        "spatie/laravel-fractal": "^5.8"
    },
    "autoload": {
        "psr-4": {
            "Scpigo\\Laravel1cXml\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Maksim Rastorguev"
        }
    ],
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "path",
            "url": "packages/scpigo/laravel-system-job"
        },
        {
            "type": "path",
            "url": "packages/scpigo/core"
        }
    ]

}
